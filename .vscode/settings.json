{
    "javascript.validate.enable": false,
    "typescript.validate.enable": false,
    "stylelint.enable": false,
    "eslint.enable": true,
    "files.exclude": {
        "admin": true,
        "app": true,
        "component": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.detectIndentation": false,
    "editor.tabSize": 4,
    "editor.wordWrap": "on"
}